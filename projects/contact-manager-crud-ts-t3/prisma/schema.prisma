// Define your datasource and generator
 datasource db {
   provider = "sqlite"
   url      = "file:./dev.db"
 }
 
 generator client {
   provider = "prisma-client-js"
 }
 
 // Define your Contact model
 model Contact {
   id    Int    @id @default(autoincrement())
   name  String
   email String @unique
   phone String
 }
